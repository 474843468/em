  </style>

  <style name="OverBillCollectionContentFragmentTvRight"
      parent="@style/WrapWidthAndWrapHeight">
    <item name="android:layout_alignParentRight">true</item>
    <item name="android:layout_centerVertical">true</item>
  </style>

  <!--MemberCentre left 会员中心左侧-->
