    <item name="android:layout_marginTop">@dimen/margin_2_5</item>
    <item name="android:layout_marginLeft">@dimen/margin_2_5</item>
  </style>

  <!--OverBillDetailsContentFragment-->
  <style name="OverBillDetailsContentFragmentTv"
      parent="@style/WrapWidthAndWrapHeight">
    <item name="android:layout_centerVertical">true</item>
    <item name="android:textColor">@color/primary_text</item>
    <item name="android:textSize">@dimen/text_font_size_small</item>
  </style>

  <style name="OverBillDetailsContentFragmentDivider"
      parent="DividerLineGray">
    <item name="android:layout_marginBottom">@dimen/margin_1_5</item>
    <item name="android:layout_marginTop">@dimen/margin_1_5</item>
  </style>
  <!--通用 TableTypeAdapter 餐桌分类 item-->
  <style name="ItemTableTypeStyle"
      parent="@style/WrapWidthAndWrapHeight">
    <item name="android:background">@drawable/selector_tv_table_type</item>
    <item name="android:gravity">center</item>
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:minHeight">?android:attr/listPreferredItemHeightSmall</item>
    <item name="android:textColor">@color/bill_cate_item_textcolor</item>
    <item name="android:textSize">@dimen/text_font_size_small</item>
  </style>

