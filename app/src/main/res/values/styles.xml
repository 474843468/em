    <item name="android:textSize">@dimen/text_font_size_big</item>
  </style>

  <style name="MemberCentreActivityPageTurning">
    <item name="android:padding">@dimen/padding_small</item>
    <item name="android:layout_width">wrap_content</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:textColor">@color/black</item>
    <item name="android:textSize">@dimen/text_font_size_small</item>
    <item name="android:drawablePadding">@dimen/padding_drawable</item>
